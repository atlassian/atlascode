{
  "request": {
    "method": "POST",
    "urlPathPattern": "/site/oauth2/access_token"
  },
  "response": {
    "status": 200,
    "jsonBody": {
        "access_token": "mocked_token",
        "token_type": "bearer",
        "expires_in": 3600,
        "refresh_token": "mocked_refresh_token"
    },
    "headers": {
      "Content-Type": "application/json"
    }
  }
}