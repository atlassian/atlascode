{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Start RovoDev Server",
            "type": "shell",
            "command": "acli rovodev serve 8899",
            "isBackground": true,
            "problemMatcher": {
                "pattern": {
                    "regexp": "^.*$",
                    "file": 1,
                    "location": 2,
                    "message": 3
                },
                "background": {
                    "activeOnStart": true,
                    "beginsPattern": ".*",
                    "endsPattern": ".*"
                }
            }
        }
    ]
}
